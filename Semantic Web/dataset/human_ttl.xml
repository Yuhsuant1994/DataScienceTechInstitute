@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@base <http://www.inria.fr/2007/09/11/humans.rdfs>.
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

<#Animal>
  a rdfs:Class ;
  rdfs:label "animal"@en, "animal"@fr ;
  rdfs:comment """
    a living organism characterized by voluntary movement.
    """@en, "être vivant doué de sensibilite de mobilité."@fr .

<#Male>
  a rdfs:Class ;
  owl:disjointWith <#Female>;
  rdfs:subClassOf <#Animal> ;
  rdfs:label "male"@en, "mâle"@fr ;
  rdfs:comment """
        an animal that produces gametes (spermatozoa) that can fertilize female gametes (ova).
    """@en, """
        individu appartenant au sexe qui possède le pouvoir de fécondation.
    """@fr .

<#Female>
  a rdfs:Class ;
  rdfs:subClassOf <#Animal> ;
  rdfs:label "female"@en, "femelle"@fr ;
  rdfs:comment """
        an animal that produces gametes (ova) that can be fertilized by male gametes (spermatozoa).
    """@en, """
        animal appartenant au sexe apte à produire des ovules.
    """@fr .

<#Man>
  a rdfs:Class ;
  rdfs:subClassOf <#Person>, <#Male> ;
  rdfs:label "man"@en, "homme"@fr ;
  rdfs:comment "an adult male person"@en, "mâle adulte de l'espèce humaine."@fr .

<#Person>
  a rdfs:Class;
  owl:equivalentClass  [a owl:Restriction; 
    owl:onProperty <#hasParent>;
    owl:someValuesFrom <#Woman>];
  rdfs:subClassOf <#Animal> ;
  rdfs:label "person"@en, "human being"@en, "human"@en, "personne"@fr, "être humain"@fr, "humain"@fr, "homme"@fr ;
  rdfs:comment "a member of the human species"@en, "un membre de l'espèce humaine."@fr .

<#Lecturer>
  a rdfs:Class, owl:Class ;
  rdfs:subClassOf <#Person> ;
  rdfs:label "lecturer"@en, "professeur"@fr ;
  rdfs:comment "someone who lectures professionally"@en, """
personne qui enseigne une discipline, une technique, un art.
"""@fr .

<#Researcher>
  a rdfs:Class, owl:Class ;
  rdfs:subClassOf <#Person> ;
  rdfs:label "researcher"@en, "scientist"@en, "chercheur"@fr, "scientifique"@fr ;
  rdfs:comment "a person who devotes himself to doing research"@en, "personne adonnée à des recherches spécialisées."@fr .

<#Professor>
  a rdfs:Class, owl:Class;
  owl:intersectionOf(<#Lecturer> <#Researcher>);
  rdfs:subClassOf  [a owl:Restriction; 
    owl:onProperty <#mainEmployer>;
    owl:allValuesFrom <#University>].

<#Academic>
  a rdfs:Class, owl:Class;
  owl:unionOf ( <#Lecturer> <#Researcher> ).

<#Woman>
  a rdfs:Class;
  rdfs:subClassOf <#Person>, <#Female> ;
  rdfs:label "woman"@en, "femme"@fr ;
  rdfs:comment "an adult female person"@en, "femelle adulte de l'espèce humaine."@fr .

<#Organization> a rdfs:Class .

<#University> a rdfs:Class ;
    rdfs:subClassOf <#Organization>.



<#Vegitable> a owl:Class;
  owl:disjointUnionOf ( <#Fruit> <#Meat> ).



<#mainEmployer> a rdf:Property;
    rdfs:domain <#Person>;
    rdfs:range <#Organization>.
    
<#hasAncestor>
  a rdf:Property, owl:TransitiveProperty ;
  rdfs:domain <#Animal> ;
  rdfs:range <#Animal> ;
  rdfs:label "has for ancestor"@en, "a pour ancêtre"@fr ;
  rdfs:comment """
    relation between an animal and another animal from which it is descended.
    """@en, """
    relation entre un animal et un autre animal duquel il descend.
    """@fr .

<#hasParent>
  a rdf:Property ;
  rdfs:subPropertyOf <#hasAncestor> ;
  rdfs:label "has for parent"@en, "a pour parent"@fr ;
  rdfs:comment """
    relation between an animal and another animal which gave birth to it.
    """@en, """
    relation entre un animal et un autre animal qui lui a donné naissance.
    """@fr .

<#olderThan>
  a rdf:Property ;
  rdfs:domain <#Animal> ;
  rdfs:range <#Animal> .

<#hasChild>
  a rdf:Property ;
  owl:inverseOf <#hasParent> ;
  rdfs:label "has for child"@en, "a pour enfant"@fr ;
  rdfs:comment """
    relation between an animal and another animal to which it gave birth.
    """@en, """
    relation entre un animal et un autre animal auquel il a donné naissance.
    """@fr .

<#hasFather>
  a rdf:Property ;
  rdfs:subPropertyOf <#hasParent> ;
  rdfs:range <#Male> ;
  rdfs:label "has for father"@en, "a pour père"@fr ;
  rdfs:comment "to have for parent a male."@en, "avoir pour parent un mâle."@fr .

<#hasMother>
  a rdf:Property ;
  rdfs:subPropertyOf <#hasParent> ;
  rdfs:range <#Female> ;
  rdfs:label "has for mother"@en, "a pour mère"@fr ;
  rdfs:comment "to have for parent a female."@en, "avoir pour parent un femmelle."@fr .

<#hasBrother>
  a rdf:Property ;
  rdfs:domain <#Animal> ;
  rdfs:range <#Male> ;
  rdfs:label "has for brother"@en, "a pour frère"@fr ;
  rdfs:comment "relation with a male who has the same parents."@en, "relation avec un mâle ayant les mêmes parents."@fr .

<#hasSister>
  a rdf:Property ;
  rdfs:domain <#Animal> ;
  rdfs:range <#Female> ;
  rdfs:label "has for sister"@en, "a pour soeur"@fr ;
  rdfs:comment "relation with a female who has the same parents."@en, "relation avec une femelle ayant les mêmes parents."@fr .

<#hasFriend> a rdf:Property, owl:SymmetricProperty ;
  rdfs:domain <#Person> ;
  rdfs:range <#Person> ;
  rdfs:label "has for friend"@en, "a pour ami"@fr ;
  rdfs:comment """
    relation between a person and another person he or she knows well and regards with affection and trust.
    """@en, """
    relation entre une personne et une autre personne qui est l'objet d'un attachement privilégié.
    """@fr .

<#name>
  a rdf:Property ;
  rdfs:label "name"@en, "nom"@fr ;
  rdfs:comment "designation of something."@en, "désignation de quelque chose."@fr .

<#shoesize>
  a rdf:Property ;
  rdfs:domain <#Person> ;
  rdfs:label "shoe size"@en, "size"@en, "pointure"@fr ;
  rdfs:comment """
express in some way the approximate length of the shoes for a person.
"""@en, """
taille, exprimée en points, des chaussures d'une personne.
"""@fr .

<#age>
  a rdf:Property ;
  rdfs:label "age"@en, "âge"@fr ;
  rdfs:comment "complete existence duration."@en, "durée complète d'existence."@fr .

<#shirtsize>
  a rdf:Property ;
  rdfs:domain <#Person> ;
  rdfs:label "shirt size"@en, "size"@en, "taille de chemise"@fr, "taille"@fr ;
  rdfs:comment """
express in some way the approximate dimensions of the shirts of a person.
"""@en, """
dimensions approximatives des chemises portées par une personne.
"""@fr .

<#trouserssize>
  a rdf:Property ;
  rdfs:domain <#Person> ;
  rdfs:label "trousers size"@en, "size"@en, "taille de pantalon"@fr, "taille"@fr ;
  rdfs:comment """
    express in some way the approximate dimensions of the trousers of a person.
    """@en, """
    dimensions approximatives des pantalons portés par une personne.
    """@fr .

<#hasSpouse> a rdf:Property, owl:SymmetricProperty ;
  rdfs:domain <#Person> ;
  rdfs:range <#Person> ;
  rdfs:label "has for spouse"@en, "est en ménage avec"@fr ;
  rdfs:comment "a person's partner in marriage."@en, "le partenaire d'une personne dans un mariage."@fr .